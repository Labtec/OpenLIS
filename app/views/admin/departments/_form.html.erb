<%= form_with(model: [:admin, @department], data: { turbo: false }) do |form| %>
  <%= render 'shared/error_messages', object: @department %>
  <p>
    <%= form.label :name %><br />
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :code %><br />
    <%= form.text_field :code %>
  </p>
  <p>
    <%= form.label :loinc_class %><br />
    <%= form.text_field :loinc_class %>
  </p>
  <p><%= form.submit 'Submit' %></p>
<% end %>
