<% title "Lab Test Values" %>

<table>
  <tr>
    <th>Value</th>
    <th>Flag</th>
    <th>Note</th>
  </tr>
  <% for lab_test_value in @lab_test_values %>
    <tr>
      <td><%=h sanitize lab_test_value.value %></td>
      <td><%=h lab_test_value.flag %></td>
      <td><%=h sanitize lab_test_value.note %></td>
      <td><%= link_to "Show", [:admin, lab_test_value] %></td>
      <td><%= link_to "Edit", edit_admin_lab_test_value_path(lab_test_value) %></td>
      <td><%= link_to "Destroy", [:admin, lab_test_value], :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Lab Test Value", new_admin_lab_test_value_path %></p>
