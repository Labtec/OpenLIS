<%= form_with(model: [:admin, @insurance_provider]) do |form| %>
  <%= render 'shared/error_messages', object: @insurance_provider %>
  <p>
    <%= form.label :name %><br />
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :price_list_id %><br />
    <%= form.collection_select :price_list_id, PriceList.active, :id, :name, { include_blank: true } %>
  </p>

  <div class="buttonbar">
    <%= form.submit 'Submit' %> |
    <%= link_to cancel, admin_insurance_providers_path %>
  </div>
<% end %>
