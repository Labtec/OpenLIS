<%= form_with(model: [:admin, @doctor], data: { turbo: false }) do |form| %>
  <%= render 'shared/error_messages', object: @doctor %>
  <p>
    <%= form.label :name %><br />
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :email %><br />
    <%= form.email_field :email %>
  </p>
  <p><%= form.submit 'Submit' %></p>
<% end %>
